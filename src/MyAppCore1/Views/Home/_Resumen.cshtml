@model Restaurante

<section class="panel panel-default">
    <h3 class="panel-heading">@Model.Nombre</h3>
    <div class="panel-body">
        Cosina: @Model.Cosina
    </div>
    <div class="panel-footer">
        <a class="btn btn-primary" asp-action="Details" asp-route-id="@Model.Id">Detalles</a>
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
    </div>
</section>